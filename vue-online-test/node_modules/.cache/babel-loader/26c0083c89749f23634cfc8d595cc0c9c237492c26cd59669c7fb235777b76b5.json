et(`http://localhost:4399/question/getQuestion?currentPage=${currentPage}&pageSize=${pageSize}`)\r\n                .then((res) => {\r\n                    this.questionData = res.data.data.source;\r\n                })\r\n        },\r\n        openAddDialog() {\r\n            // 打开添加对话框\r\n            this.addDialogVisible = true;\r\n        },\r\n\r\n        closeAddDialog() {\r\n            // 关闭添加对话框\r\n            this.addDialogVisible = false;\r\n        },\r\n        saveQuestionData() {\r\n            // ... 其他逻辑\r\n            axios({\r\n                method:'put',\r\n                data:this.addFormData\r\n            })\r\n            // 请求成功后关闭对话框\r\n            this.dialogFormVisible = false;\r\n            this.addDialogVisible = false;\r\n        },\r\n    },\r\n    mounted() {\r\n        axios({\r\n            method: 'get',\r\n            url: 'http://localhost:4399/question/getQuestion'\r\n        }).then(res => {\r\n            this.questionData = res.data.data.source;\r\n            console.log(this.questionData);\r\n        })\r\n    }\r\n}\r\n</script>\r\n\r\n<style></style>"],"mappings":"AA8FA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,YAAA;MACAC,QAAA;MACAC,UAAA;MACAC,gBAAA;MACAC,WAAA;QACA;QACAC,OAAA;QACAC,IAAA;QACAC,GAAA;QACAC,GAAA;QACAC,GAAA;QACAC,GAAA;QACAC,aAAA;QACA;MACA;IACA;EACA;;EACAC,OAAA;IACAC,iBAAAC,IAAA;MACA,MAAAC,WAAA,GAAAD,IAAA;MACA,MAAAb,QAAA,QAAAA,QAAA;MAEAH,KAAA,CAAAkB,GAAA,2DAAAD,WAAA,aAAAd,QAAA,IACAgB,IAAA,CAAAC,GAAA;QACA,KAAAlB,YAAA,GAAAkB,GAAA,CAAAnB,IAAA,CAAAA,IAAA,CAAAoB,MAAA;MACA;IACA;IACAC,cAAA;MACA;MACA,KAAAjB,gBAAA;IACA;IAEAkB,eAAA;MACA;MACA,KAAAlB,gBAAA;IACA;IACAmB,iBAAA;MACA;MACAxB,KAAA;QACAyB,MAAA;QACAxB,IAAA,OAAAK;MACA;MACA;MACA,KAAAoB,iBAAA;MACA,KAAArB,gBAAA;IACA;EACA;EACAsB,QAAA;IACA3B,KAAA;MACAyB,MAAA;MACAG,GAAA;IACA,GAAAT,IAAA,CAAAC,GAAA;MACA,KAAAlB,YAAA,GAAAkB,GAAA,CAAAnB,IAAA,CAAAA,IAAA,CAAAoB,MAAA;MACAQ,OAAA,CAAAC,GAAA,MAAA5B,YAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}